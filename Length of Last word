class Solution {
    public int lengthOfLastWord(String s) {
        int count =0;
        int flag =0;
        for(int i=s.length()-1; i>=0; i--){
            char ch = s.charAt(i);
            if(ch==' ' && flag!=0)
            break;

            else if(ch==' '){
                
            continue;
            }

            else{
                flag++;
            count++;}
        }

        return count;
    }
}
